{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "unicorn"],
  "rules": {
    "unicorn/filename-case": ["error", {
      "case": "camelCase"
    }],
    "react/react-in-jsx-scope": "off"
  }
}